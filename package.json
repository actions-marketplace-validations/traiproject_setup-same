{
  "name": "setup-same",
  "version": "1.0.0",
  "description": "GitHub Action to setup the same CLI tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest"
  },
  "keywords": ["github", "action", "same", "cli", "setup"],
  "author": "Traiproject",
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.2.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^24.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.11",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.19.1",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "prettier": "^3.2.4"
  }
}
